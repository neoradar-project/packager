{
  "$schema": "https://raw.githubusercontent.com/neoradar-project/schemas/main/package/mapStyle.schema.json",
  "inactiveSectorBackgroundColor": "#000000",
  "gateLabelColor": "#FFFFFF",
  "sidStyle": {
    "color": "#D87E00",
    "width": 1,
    "dashArray": [
      2,
      1
    ]
  },
  "starStyle": {
    "color": "#0E00D8",
    "width": 1,
    "dashArray": [
      3,
      2
    ]
  },
  "sectorLineStyle": {
    "color": "#404040",
    "width": 1,
    "dashArray": [
      1,
      3
    ]
  },
  "activeSectorFillStyle": {
    "color": "#000000",
    "opacity": 1
  },
  "activeSectorLineStyle": {
    "color": "#000000",
    "width": 2,
    "dashArray": [
      1
    ]
  },
  "otherSectorLineStyle": {
    "color": "#ff6200",
    "width": 2,
    "dashArray": [
      1
    ]
  },
  "gateInactiveStyle": {
    "outlineColor": "#555555",
    "outlineWidth": 1,
    "outlineOpacity": 0.7,
    "fillColor": "#6B6B6B",
    "fillOpacity": 0.5
  },
  "gateOccupiedStyle": {
    "outlineColor": "#EDC218",
    "outlineWidth": 1,
    "outlineOpacity": 0.7,
    "fillColor": "#EDC218",
    "fillOpacity": 0.5
  },
  "gateBookedStyle": {
    "outlineColor": "#1462FF",
    "outlineWidth": 1,
    "outlineOpacity": 0.7,
    "fillColor": "#1462FF",
    "fillOpacity": 0.5
  }
}
